{
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNetCore.IISPlatformHandler": "1.0.0-*",
    "Microsoft.AspNetCore.Localization": "1.0.0-*",
    "Microsoft.Extensions.Localization": "1.0.0-*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*"
  },

  "compilationOptions": {
    "emitEntryPoint": true
  },

  "commands": {
    "web": "LocalizationSample"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
